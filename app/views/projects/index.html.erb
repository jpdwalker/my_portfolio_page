<h1 id="page-title">Projects</h1>
<div id="posts-wrapper" class="skinny-wrapper">
  <% @projects.each do |project| %>
    <div class="post">
      <p class="date"><%= project.created_at.strftime('%B %d, %Y') %></p>
      <h2><%= link_to project.title, project %></h2>
      <hr>
    </div>
  <% end %>

</div>

 <%#= will_paginate @articles %>
