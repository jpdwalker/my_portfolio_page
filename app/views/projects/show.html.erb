<div id="post_show_content" class="skinny-wrapper wrapper_padding">
  <header>
    <p class="date"><%=  @project.created_at.strftime("%b %d, %Y" ) %></p>
    <h1><%= @project.title %></h1>
    <hr>
  </header>

  <div id="content">
    <p><%= markdown @project.description %></p>
    <div class="button_wrapper">
      <a href="<%= @project.link %>" class="project_button" target="_blank">Visit <%= @project.title %></a>
    </div>
  </div>

</div>
