<h1 id="page-title">Writings</h1>
<div id="posts-wrapper" class="skinny-wrapper">
  <% @articles.each do |article| %>
    <div class="post">
      <p class="date"><%= article.created_at.strftime('%B %d, %Y') %></p>
      <h2><%= link_to article.title, article %></h2>
      <hr>
    </div>
  <% end %>

</div>

 <%= will_paginate @articles %>
